{
  "name": "modules-http-stubs",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/modules/http-stubs/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/modules/http-stubs",
        "main": "libs/modules/http-stubs/src/index.ts",
        "tsConfig": "libs/modules/http-stubs/tsconfig.lib.json",
        "assets": ["libs/modules/http-stubs/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/modules/http-stubs/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/modules/http-stubs/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": ["modules"]
}
